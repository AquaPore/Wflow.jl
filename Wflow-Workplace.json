// Colours theme start
"editor.tokenColorCustomizations": {

    "textMateRules": [
        {
            "scope": "source.julia punctuation.separator",
            "settings": {
                "foreground": "#ec7e24"
            }
        } ,

         {
              "scope": [
                    "comment",
                    "string.comment"
              ],
              "settings": {
                    "foreground": "#79b85c", // "#7cb661" #6A9955 #7CA668 #229977
                    "fontStyle": "italic bold",
              }
         },

         {
              "scope": "constant.numeric",
              "settings": {
                    "foreground": "#AE81FF",
                    "fontStyle": "italic"
              }
         },
    ]

},

